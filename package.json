{
  "name": "ghostreconwildlands-resourcemanager",
  "productName": "GhostReconWildlands-ResourceManager",
  "version": "0.0.2",
  "description": "This app is for sending resources to your Ghost Recon Wildlands game",
  "license": "GPL-3.0",
  "repository": "https://github.com/AdrianoCahete/grw-resourcemanager",
  "author": {
    "name": "Adriano Cahet√©",
    "email": "",
    "url": "https://www.profolio.com.br"
  },
  "scripts": {
    "preinstall": "npm install -g electron-packager",
    "test": "xo",
    "start": "electron .",
    "build": "electron-packager . --out=dist --asar --overwrite --platform win32 --arch all",
    "pack": "build --dir",
    "dist": "build"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css",
    "res/*",
    "grwresources.js"
  ],
  "build": {
    "appId": "br.com.profolio.grwapp"
  },
  "keywords": [
    "ghost recon",
    "ghostrecon",
    "wildlands",
    "resource manager",
    "pcmr",
    "grw"
  ],
  "dependencies": {
    "electron-debug": "^1.0.0"
  },
  "devDependencies": {
    "devtron": "^1.1.0",
    "electron": "^1.3.3",
    "electron-packager": "^8.0.0",
    "electron-winstaller": "^2.5.2",
    "xo": "^0.16.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  }
}
